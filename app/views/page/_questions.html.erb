<ul class="nav questions__list">
  <%- questions.each do |q| %>
    <li>
      <div class="card">
        <div class="card__body">
          <h5>
            <%= link_to q.title, question_path(q.id) %>
          </h5>
          <p>
            <%= q.description %>
          </p>
          <em>
            <%= q.user.email %>
          </em>
        </div>
      </div>
    </li>
  <%-end %>
</ul>
