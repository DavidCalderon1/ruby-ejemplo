<ul class="nav questions__list">
  <%- questions.each do |q| %>
    <li>
      <div class="card">
        <div class="card__body">
          <h3 class='title'>
            <%= link_to q.title, question_path(q.id) %>
          </h3>
					<p class= "date">
						Última actualización: 
						<%= q.updated_at %>
					</p>
          <p>
            <%= q.description %>
          </p>
          <em>
            <%= q.user.email %>
          </em>
        </div>
      </div>
    </li>
  <%-end %>
</ul>
